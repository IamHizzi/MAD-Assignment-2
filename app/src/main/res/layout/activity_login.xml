<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Login">
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="text"
            android:ems="10"
            android:id="@+id/editText6"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="99dp" android:layout_marginStart="99dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="99dp" android:layout_marginRight="99dp" android:layout_marginTop="136dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="30dp"
            app:layout_constraintBottom_toTopOf="@+id/editText7" android:hint="Name"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editText7"
            android:hint="Age"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="99dp" android:layout_marginStart="99dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="99dp" android:layout_marginRight="99dp" android:layout_marginTop="50dp"
            app:layout_constraintTop_toBottomOf="@+id/editText6" android:layout_marginBottom="100dp"
            app:layout_constraintBottom_toTopOf="@+id/button3"/>
    <Button
            android:text="SignIn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="Welcome"
            android:id="@+id/button3" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="252dp"
            android:layout_marginStart="252dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="171dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="71dp" android:layout_marginRight="71dp" tools:ignore="OnClick"
            android:layout_marginTop="110dp" app:layout_constraintTop_toBottomOf="@+id/editText7"/>
</androidx.constraintlayout.widget.ConstraintLayout>